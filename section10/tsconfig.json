{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "outDir": "dist",
        "strict": true,
        "moduleDetection": "force",
        "skipLibCheck": true,
        "allowJs": true, // js 파일도 허용
        "noImplicitAny": true, // 암시적 Any 타입을 허용하지 않는 옵션
    },
    "include": ["src"]
}